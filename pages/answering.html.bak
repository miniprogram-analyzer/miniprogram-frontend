<!doctype html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>代码分析平台</title>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=EDGE" />
    <meta name="description" />
    <meta name="keywords" />
    <meta http-equiv="pragma" content="no-cache">
    <meta http-equiv="Cache-Control" content="no-cache, must-revalidate">
    <meta http-equiv="expires" content="0">
    <meta name="format-detection" content="telephone=no" />
    <link href="/lib/site.min.css" rel="stylesheet" type="text/css" />
    <link href="/lib/index.min.css" rel="stylesheet" type="text/css" />
    <link href="/lib/style.min.css" rel="stylesheet" type="text/css" />
    <link href="/lib/navis.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <script src="/lib/darkmode.js"></script>
    <script src="/lib/navigation.js"></script>
    <script>const nav_id = 3;</script>

    <div class="bb" style="margin-top: 106px;">
        <div id="navis">
            <ul class="bar">
                <li data-id="1" id="b-1" class="sel"><a href="#buglist">问题列表</a></li>
                <li data-id="2" id="b-2" class=""><a href="#faq">问题详情</a></li>
                <!--<li data-id="3" id="b-3" class=""><a href="#exp">经验分享</a></li>-->
                <span class="extra-bar">
                    <a href="/asking">去提问</a>
                    <!--<a href="/pages/asking_richtext.html">去提问(富文本)</a>-->
                </span>
            </ul>
            <div class="bd sel" id="d-1">
                <iframe class="vue-iframe" id="vue-buglist" src="/pages_test/vue_discuss.html" scrolling="no"
                    style="height:1000px"></iframe>
            </div>
            <div class="bd" id="d-2">
                <iframe class="vue-iframe" id="vue-buglist" src="/pages_test/vue_discussion.html" scrolling="no"
                    style="height:1000px"></iframe>
            </div>
            <!--<div class="bd" id="d-2">
                <img src="\img\building.jpg" style="width:100%">
            </div>
            <div class="bd" id="d-3">
                <img src="\img\building.jpg" style="width:100%">
            </div>-->
        </div>
    </div>

    <script src="/jquery/jquery.min.js"></script>
    <script>
        (window.jQuery || document.write("<script src='\/jquery\/jquery.min.js'><\/script>"));
    </script>
    <script src="/jquery/jquery.cookie.min.js"></script>
    <script>
        (window.jQuery && window.jQuery.fn && window.jQuery.fn.cookie || document.write(
            "\u003Cscript src=\u0022/jquery/jquery.cookie.min.js\u0022\u003E\u003C\/script\u003E"));
    </script>
    <script src="/jquery/jquery.lazyload.min.js"></script>
    <script>
        (window.jQuery && window.jQuery.fn && window.jQuery.fn.lazyload || document.write(
            "\u003Cscript src=\u0022/jquery/jquery.lazyload.min.js\u0022\u003E\u003C\/script\u003E"
        ));
    </script>
    <script src="/lib/popwin.js"></script>
    <script src="/lib/site.js"></script>
    <script>
        var para = window.location.hash
        const pages = ["buglist", "faq", "exp"]
        var page = pages.indexOf(para.split("#")[1]) + 1
        console.log(page)
        if (page <= 0) {
            page = 1;
        }
        var idb = "#b-" + page
        var idd = "#d-" + page
        $("#navis .bar li").removeClass("sel");
        $("#navis .bd").removeClass("sel");
        $(idb).addClass("sel");
        $(idd).addClass("sel");
        $("#navis .bar li a").click(function () {
            $("#navis .bar li").removeClass("sel");
            $(this).parent().addClass("sel");

            var id = "#d-" + $(this).parent().data("id");
            $("#navis .bd").removeClass("sel");
            $(id).addClass("sel");
        });
        /*$("#vue-buglist").on("load", function () {
            $("#vue-buglist").css("background-image", "none");
        })*/
        if (nightMode) {
            $("#vue-buglist").attr("src","/pages/vue_search.html?night=1")
        }
    </script>
    <script src="/lib/footer.js"></script>
</body>

</html>